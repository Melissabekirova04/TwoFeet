<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="520.0" style="-fx-background-color: #02213A;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.FrontPageController">
    <children>
        <AnchorPane layoutX="200.0" prefHeight="147.0" prefWidth="200.0">
            <children>
                <ImageView fx:id="logoImageView" fitHeight="181.0" fitWidth="182.0" layoutX="-17.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Logo1.png" />
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <Label layoutX="100.0" layoutY="145.0" text="Welcome to" textFill="#f49338">
            <font>
                <Font name="Calibri Bold" size="18.0" />
            </font>
            <effect>
                <DropShadow blurType="TWO_PASS_BOX" height="30.47" radius="17.1025" width="39.94" />
            </effect>
        </Label>
        <Button fx:id="firstRegisterButton" layoutX="146.0" layoutY="217.0" mnemonicParsing="false" onAction="#firstRegisterButtonOnAction" prefHeight="25.0" prefWidth="218.0" style="-fx-background-color: #F49338;" text="Register" textFill="WHITE">
            <font>
                <Font name="Calibri" size="13.0" />
            </font>
        </Button>
        <Button fx:id="firstLoginButton" layoutX="146.0" layoutY="281.0" mnemonicParsing="false" onAction="#firstLoginButtonOnAction" prefHeight="25.0" prefWidth="218.0" style="-fx-background-color: #F49338;" text="Login" textFill="WHITE" />
        <Label layoutX="146.0" layoutY="192.0" text="New User? Register here" textFill="WHITE">
            <font>
                <Font name="Calibri" size="13.0" />
            </font>
        </Label>
        <Label layoutX="146.0" layoutY="257.0" text="Already a User? Login here" textFill="WHITE">
            <font>
                <Font name="Calibri" size="13.0" />
            </font>
        </Label>
        <Button fx:id="firstCloseButton" layoutX="146.0" layoutY="331.0" mnemonicParsing="false" onAction="#firstCloseButtonOnAction" prefHeight="25.0" prefWidth="218.0" style="-fx-background-color: #fffff;" text="Close" textFill="#f49338" />
    </children>
</AnchorPane>
