<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="844.0" prefWidth="390.0" style="-fx-background-color: #02213A;" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.FrontPageController">
    <children>
          <ImageView fx:id="logoImageView" fitHeight="181.0" fitWidth="182.0" layoutX="105.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true">
              <image>
                  <Image url="@Logo1.png" />
              </image>
          </ImageView>
        <Label layoutX="22.0" layoutY="161.0" text="Welcome to" textFill="#f49338">
            <font>
                <Font name="Calibri Bold" size="18.0" />
            </font>
            <effect>
                <DropShadow blurType="TWO_PASS_BOX" height="30.47" radius="17.1025" width="39.94" />
            </effect>
        </Label>
        <Button fx:id="firstLoginButton" layoutX="85.0" layoutY="456.0" mnemonicParsing="false" onAction="#firstLoginButtonOnAction" prefHeight="60.0" prefWidth="218.0" style="-fx-background-color: #F49338;" text="Login" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font></Button>
        <Label layoutX="67.0" layoutY="422.0" text="Already a User? Login here" textFill="WHITE">
            <font>
                <Font name="Calibri" size="18.0" />
            </font>
        </Label>
        <Button fx:id="firstCloseButton" layoutX="85.0" layoutY="770.0" mnemonicParsing="false" onAction="#firstCloseButtonOnAction" prefHeight="13.0" prefWidth="218.0" style="-fx-background-color: #fffff;" text="Close">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font></Button>
        <Label layoutX="67.0" layoutY="269.0" text="New User? Register here" textFill="WHITE">
            <font>
                <Font name="Calibri" size="18.0" />
            </font>
        </Label>
        <Button fx:id="firstRegisterButton" layoutX="85.0" layoutY="299.0" mnemonicParsing="false" onAction="#firstRegisterButtonOnAction" prefHeight="60.0" prefWidth="218.0" style="-fx-background-color: #F49338;" text="Register" textFill="WHITE">
            <font>
                <Font name="Calibri Bold" size="18.0" />
            </font>
        </Button>
      <Button fx:id="skipToMainButton" layoutX="87.0" layoutY="616.0" mnemonicParsing="false" onAction="#skipToMainButtonOnAction" prefHeight="60.0" prefWidth="218.0" style="-fx-background-color: #F49338;" text="SKIP TO MAIN MENU" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
